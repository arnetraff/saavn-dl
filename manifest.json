{


  "description": "Demonstrating webRequests",
  "manifest_version": 2,
  "name": "webRequest-demo",
  "version": "1.0",

  "permissions": [
    "webRequest",
    "<all_urls>"
  ],


 "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content_scr.js"]
    }
  ],

  "background": {
    "scripts": ["background.js"]
  }


}